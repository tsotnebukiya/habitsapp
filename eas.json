{
  "cli": {
    "version": ">= 10.2.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development"
      },
      "ios": {
        "image": "macos-sonoma-14.6-xcode-16.1",
        "cocoapods": "1.16.2"
      }
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "image": "macos-sonoma-14.6-xcode-16.1",
        "cocoapods": "1.16.2"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
